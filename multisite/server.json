{
    "name":"commandbox-multi-site",
    "web":{
        "accessLogEnable":"true",
        "aliases":{
            "/js":"javascript"
        },
        "allowedExt":"log",
        "errorPages":{
            "404":"404.cfm"
        },
        "GZIPEnable":"true",
        "gzipPredicate":"regex('^.*\\.txt$')",
        "mimeTypes":{
            "log":"text/plain"
        },
        "rules":[
            "path(/tea)->set-error(418)"
        ],
        "rulesFile":".rules.txt",
        "welcomeFiles":"custom.cfm,index.cfm",
        "bindings":{
            "HTTP":{
                "listen":"80"
            }
        }
    },
    "siteConfigFiles":"sites-enabled/*.json",
    "hostAlias":[
        "siteMissing.com",
        "www.siteMissing.com"
    ],
    "service":{
        "serviceName":"BOX_Multisite",
        "displayName":"BOX_Multisite",
        "rotateLog":true,
        "standardOutPath":"logs/service_out.log",
        "errorOutPath":"logs/service_error.log"
    },
    "cfconfig":{
        "web-site1":"site1/.cfconfig-site1-web.json",
        "web-site2":"site2/.cfconfig-site2-web.json",
        "web-site3":"site3/.cfconfig-site3-web.json"
    }
}