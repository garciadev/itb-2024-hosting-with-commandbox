{
    "adminSalt":"12E6B8FBD9F2E15F8D0F2194F8546381",
    "applicationListener":"mixed",
    "applicationMode":"curr2root",
    "applicationTimeout":"2,0,0,0",
    "baseComponent":"/lucee/Component.cfc",
    "cacheDefaultFile":"",
    "cacheDefaultFunction":"",
    "cacheDefaultHTTP":"",
    "cacheDefaultInclude":"",
    "cacheDefaultObject":"default",
    "cacheDefaultQuery":"${LUCEE_CACHE_DEFAULT_QUERY:query}",
    "cacheDefaultResource":"",
    "cacheDefaultTemplate":"",
    "cacheDefaultWebservice":"",
    "caches":{
        "default":{
            "class":"lucee.runtime.cache.ram.RamCache",
            "custom":{
                "timeToIdleSeconds":"0",
                "timeToLiveSeconds":"0"
            },
            "readOnly":"false",
            "storage":"false",
            "type":"RAM"
        },
        "query":{
            "class":"lucee.runtime.cache.ram.RamCache",
            "custom":{
                "timeToIdleSeconds":"600",
                "timeToLiveSeconds":"600"
            },
            "readOnly":"false",
            "storage":"false",
            "type":"RAM"
        },
        "${LUCEE_SESSION_STORAGE_NAME_LOCAL}":{
            "class":"lucee.runtime.cache.ram.RamCache",
            "custom":{
                "timeToIdleSeconds":"0",
                "timeToLiveSeconds":"0"
            },
            "readOnly":"false",
            "storage":"${LUCEE_SESSION_STORAGE_NAME_LOCAL_ALLOW_STORAGE}",
            "type":"RAM"
        },
        "${LUCEE_SESSION_STORAGE_NAME_REDIS}":{
            "class":"ortus.extension.cache.redis.RedisClusterCache",
            "custom":{
                "hosts":"${REDIS_CLUSTER_HOSTS:127.0.0.1}",
                "keyprefix":"${REDIS_CLUSTER_SESSION_KEY_PREFIX:lucee-cluster-session-installhub}",
                "username" : "${REDIS_CLUSTER_USERNAME}",
                "password" : "${REDIS_CLUSTER_PASSWORD}",
                "port":"${REDIS_CLUSTER_PORT:7000}",
				"useSSL" :"${REDIS_CLUSTER_USE_SSL:false}"
            },
            "readOnly":"false",
            "storage":"${LUCEE_SESSION_STORAGE_NAME_REDIS_ALLOW_STORAGE}"
        },
        "${LUCEE_QUERY_STORAGE_NAME_REDIS}":{
            "class":"ortus.extension.cache.redis.RedisClusterCache",
            "custom":{
                "hosts":"${REDIS_CLUSTER_HOSTS:127.0.0.1}",
                "keyprefix":"${REDIS_CLUSTER_QUERY_KEY_PREFIX:lucee-cluster-query-installhub}",
                "username" : "${REDIS_CLUSTER_USERNAME}",
                "password" : "${REDIS_CLUSTER_PASSWORD}",
                "port":"${REDIS_CLUSTER_PORT:7000}",
				"useSSL" :"${REDIS_CLUSTER_USE_SSL:false}"
            },
            "readOnly":"false",
            "storage":"false"
        }
    },
    "CFMappings":{},
    "clientCookies":"yes",
    "clientManagement":"no",
    "clientStorage":"${LUCEE_CLIENT_STORAGE_NAME:Cookie}",
    "componentPaths":{},
    "customTagPaths":[],
    "compression":"false",
    "datasourcePreserveSingleQuotes":"false",
    "datasources":{
        "${OFIA_DB_DATASOURCE_NAME}":{
            "allowAlter":true,
            "allowCreate":true,
            "allowDelete":true,
            "allowDrop":true,
            "allowGrant":true,
            "allowInsert":true,
            "allowRevoke":true,
            "allowSelect":true,
            "allowUpdate":true,
            "blob":"true",
            "class":"com.microsoft.jdbc.sqlserver.SQLServerDriver",
            "clob":"true",
            "connectionLimit":"-1",
            "connectionTimeout":"20",
            "custom":"DATABASENAME=${OFIA_DB_DATABASE}&sendStringParametersAsUnicode=false&SelectMethod=direct",
            "database":"${OFIA_DB_DATABASE}",
            "dbdriver":"MSSQL",
            "dsn":"jdbc:sqlserver://{host}:{port}",
            "host":"${OFIA_DB_HOST}",
            "password":"${OFIA_DB_PASSWORD}",
            "port":"${OFIA_DB_PORT}",
            "sendStringParametersAsUnicode":"false",
            "username":"${OFIA_DB_USER}",
            "validate":"false"
        }
    },
    "debuggingEnabled":"false",
