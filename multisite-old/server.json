{
    "name":"commandbox-multisite",
    "web":{
        "accessLogEnable":"true",
        "aliases":{},
        "allowedExt":"log",
        "errorPages":{},
        "GZIPEnable":"true",
        "gzipPredicate":"regex('^.*\\.txt$')",
        "mimeTypes":{
            "log":"text/plain"
        },
        "rules":[
            "path(/tea)->set-error(418)"
        ],
        "rulesFile":".rules.txt",
        "welcomeFiles":"custom.cfm,index.cfm",
        "bindings":{
            "HTTP":{
                "listen":"80"
            }
        }
    },
    "siteConfigFiles":"sites-enabled/*.json",
    "hostAlias":[
        "siteMissing.com",
        "www.siteMissing.com"
    ],
    "service":{
        "serviceName":"BOX_Multisite",
        "displayName":"BOX_Multisite",
        "rotateLog":true,
        "standardOutPath":"logs/service_out.log",
        "errorOutPath":"logs/service_error.log"
    }
}